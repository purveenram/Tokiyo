{
  "builds": [
    {
      "src": "myapp/**",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "60mb",
        "runtime": "python3.11"
      }
    }
  ],
  "functions": {},
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myapp/wsgi.py"
    }
  ],
  "env": {},
  "scripts": {
    "postinstall": "python manage.py initialize_db"
  }
}
